{
  "buildCommand": "echo 'Using pre-built Flutter web files'",
  "outputDirectory": "build/web",
  "installCommand": "echo 'No installation needed'",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
